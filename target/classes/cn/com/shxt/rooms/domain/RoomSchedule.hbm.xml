<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.shxt.rooms.domain">
	<class name="RoomSchedule" table="RJ_SCHEDULE">
		<id name="sche_id" column="SCHE_ID" type="integer">
			<generator class="sequence">
				<param name="sequence">SEQ_APP</param>
			</generator>
		</id>
		
		<property name="rrPName" column="RR_PNAME" type="string" length="32"/>
		
		<property name="scheTime" column="SCHE_TIME" type="date"/>
		
		<property name="scheDay" column="SCHE_DAY" type="integer" />
		
		<property name="scheDeposit" column="SCHE_DEPOSIT" type="integer"/>
		
		<property name="scheNum" column="sche_num" type="integer"/>
		
		<property name="scheBilling" column="sche_billing" type="string" length="32"/>
		
	</class>
</hibernate-mapping>
