<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.shxt.sys_urp.domain">
	<class name="Member" table="RJ_MEMBER">
		<id name="m_id" column="M_ID" type="integer">
			<generator class="sequence">
				<param name="sequence">SEQ_APP</param>
			</generator>
		</id>
		
		<property name="rrIdcard" column="RR_IDCARD" type="string" length="32"/>
		
		<property name="mPhone" column="M_PHONE" type="string" length="32"/>
		
		<property name="mType" column="M_TYPE" type="string" length="32"/>
		
		<property name="mRemark" column="M_REMARK" type="string" length="256"/>
		
		
	</class>
</hibernate-mapping>
